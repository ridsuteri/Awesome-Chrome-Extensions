{
    "name": "Wiki Seearch",
    "description": "searching Wikipedia",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "contextMenus"
    ],
    "icons": {
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "/images/icon16.png",
            "32": "/images/icon32.png",
            "48": "/images/icon48.png",
            "128": "/images/icon128.png"
        }
    }
}
