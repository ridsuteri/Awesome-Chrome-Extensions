{
    "name": "Type Change Color",
    "description":"Type the Color and Change it",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_title": "Type Change Color",
      "default_popup":"popup.html"
    },
    
    "options_page": "popup.html"
  }